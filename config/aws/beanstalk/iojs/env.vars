export HOME=/root
export NPM_CONFIG_LOGLEVEL=warn
export NPM_CONFIG_COLOR=false
export NPM_CONFIG_PRODUCTION=true
export IOJS_VER=1.5.0
case $( arch ) in
( i686 ) export ARCH=x86;;
( x86_64 ) export ARCH=x64;;
esac
export PATH="$PATH:/opt/elasticbeanstalk/iojs-install/iojs-v$IOJS_VER-linux-$ARCH/bin/:/root/.npm"
